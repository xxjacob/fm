	color: #3A4046;
	padding: 30px;
	/*text-align: center;*/
}

.message_text {
	font-size: 18px;
	/*line-height: 1.27;*/
	font-weight: bold;
	color: #0D2D52;
}

a {
	color: #00499C;
	text-decoration: none;
}
</style>

